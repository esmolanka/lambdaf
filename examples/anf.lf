
(let ( [swapper (lambda (s q)
                  (loop (fst snd)
                        (** (^+ snd (^ s))
                            (^+ fst (^ q))
                            (^+ fst snd))))] )
  (do (writeln "Enter S:")
      {s <- (readnum (readln))}

      (writeln "Enter Q:")
      {q <- (readnum (readln))}

      (swapper s q)))
